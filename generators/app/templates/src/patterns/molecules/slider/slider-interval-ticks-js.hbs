// Slider horizontal (default) with interval ticks init
evoSlider('evo-slider-default-ticks',{
	step: 10,
	ticks: true
});

// Slider range horizontal (default) with interval ticks init
evoSliderRange('evo-slider-range-default-ticks',{
	step: 10,
	ticks: true
});

// Slider range horizontal - fixed min with interval ticks init
evoSliderRangeSingleHandle('evo-slider-range-single-min-ticks',{
	range: 'min',
	step: 10,
	ticks: true
});

// Slider range horizontal - fixed max with interval ticks init
evoSliderRangeSingleHandle('evo-slider-range-single-max-ticks',{
	range: 'max',
	step: 10,
	ticks: true
});